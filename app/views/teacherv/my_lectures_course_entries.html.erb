<h1>Teacherv#my_lectures_course_entries</h1>
<p>Find me in app/views/teacherv/my_lectures_course_entries.html.erb</p>
<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Lectures Name</th>
      <th>Lectures Number</th>
      <th>Lectures Date</th>
      <th>Lectures Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @lecture_course_entries.each do |le| %>
    <tr>
      <td><%= le.Lecture_course.Course.level + le.Lecture_course.Course.section%></td>
      <td><%= le.Lecture_course.Lecture.name%></td>
      <td><%= le.number %></td>
      <td><%= le.date %></td>
      <td><%= le.description %></td>
      <td><%= le.id %></td>
      <td><%= link_to 'Edit', edit_lecture_course_entry_path(le.id) %></td>
      <td><%= link_to 'Attendance', teacherv_attending_path(:id => le.id)  %></td>
    </tr>
    <% end %>
  </tbody>
</table>
