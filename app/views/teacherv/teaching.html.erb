<h1>Teacherv#Teaching</h1>
<p>Find me in app/views/teacherv/teaching.html.erb</p>

  <table>
    <thead>
      <tr>
        <th>Course</th>
        <th>Lecture</th>
        <th>ID</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @lecture_courses.each do |lecture_course| %>
        <tr>
          <td><%= lecture_course.Course.level + lecture_course.Course.section %></td>
          <td><%= lecture_course.Lecture.name %></td>
          <td><%= lecture_course.id %></td>
          <td><%= link_to "add Entry", new_lecture_course_entry_path(lecture_course: lecture_course.id)%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
