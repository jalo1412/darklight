<p id="notice"><%= notice %></p>

<h1>Type Of Evaluations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_of_evaluations.each do |type_of_evaluation| %>
      <tr>
        <td><%= type_of_evaluation.name %></td>
        <td><%= type_of_evaluation.weight %></td>
        <td><%= link_to 'Show', type_of_evaluation %></td>
        <td><%= link_to 'Edit', edit_type_of_evaluation_path(type_of_evaluation) %></td>
        <td><%= link_to 'Destroy', type_of_evaluation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Of Evaluation', new_type_of_evaluation_path %>
